{
  "name": "order-monitor-ui",
  "version": "1.0.0",
  "description": "ui app for order monitor with react",
  "main": "app.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config ./webpack.prod.config.js",
    "start-win-dev": "set NODE_ENV=dev&&webpack-dev-server --progress --colors --hot --inline --content-base ./server/public --config ./webpack.config.js --display-error-detail --history-api-fallback",
    "start-win-test": "set NODE_ENV=test&&webpack-dev-server --progress --colors --hot --inline --content-base ./server/public --config ./webpack.config.js --display-error-detail --history-api-fallback",
    "start-win-prd": "set NODE_ENV=prd&&webpack-dev-server --progress --colors --hot --inline --content-base ./server/public --config ./webpack.config.js --display-error-detail --history-api-fallback",
    "start-mac-dev": "NODE_ENV=dev&&webpack-dev-server --progress --colors --hot --inline --content-base ./server/public --config ./webpack.config.js --display-error-detail --history-api-fallback",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react"
  ],
  "author": "xiangxiang kong",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "es6-promise": "^3.1.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "highcharts": "^4.2.4",
    "history": "^2.0.0",
    "img-loader": "^1.2.2",
    "isomorphic-fetch": "^2.2.1",
    "jsx-loader": "^0.13.2",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.2.1",
    "react-router": "^2.0.0",
    "react-router-redux": "^3.0.0",
    "redux": "^3.2.1",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.4",
    "redux-thunk": "^1.0.3",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
